{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" },
        { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" },
        { "key": "Cross-Origin-Resource-Policy", "value": "cross-origin" },
        { "key": "Access-Control-Allow-Origin", "value": "https://en.wikipedia.org" },
        { "key": "Content-Security-Policy", "value": "frame-ancestors 'self' https://en.wikipedia.org"}
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash":false,
  "rewrites": [
    { "source": "/fast", "destination": "/fast.html" }
  ]
  
}