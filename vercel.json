{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" },
        { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" },
        { "key": "Content-Security-Policy", "value": "frame-src https://en.wikipedia.org;"}
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash":false,
  "rewrites": [
    { "source": "/fast", "destination": "/fast.html" }
  ]
  
}